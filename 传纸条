#include <bits/stdc++.h>
using namespace std;
const int peng=50;
int m,n;
int arr[peng][peng];
int dp[peng][peng][peng][peng];

int main() {
	cin>>m>>n;
	for(int i=1;i<=m;i++){
		for(int j=1;j<=n;j++){
			cin>>arr[i][j];
		}
	}
	dp[1][1][1][1]=0;
	for(int i=1;i<=m;i++){
		for(int j=1;j<=n;j++){
			for(int k=1+i;k<=m;k++){
				for(int l=1;l<j;l++){
					dp[i][j][k][l]=max(max(dp[i][j-1][k][l-1],dp[i][j-1][k-1][l]),max(dp[i-1][j][k-1][l],dp[i-1][j][k][l-1]))+arr[i][j]+arr[k][l];
				}
			}
		}
	}
	cout<<dp[m-1][n][m][n-1]<<endl;
	return 0;
}
